# $Id: Makefile.in,v 1.4 2001/12/01 01:05:38 bbraun Exp $

prefix=@prefix@
datadir=@datadir@
srcdir=@srcdir@
top_srcdir=@top_srcdir@

INSTALL=@INSTALL@

VPATH=@srcdir@

all:

Ssh.bin:  Ssh.bin.uu
	uudecode $(srcdir)/Ssh.bin.uu

clean:
	rm -rf Ssh.bin

distprep: Ssh.bin

distclean: clean
	rm -f Makefile *~

install: $(srcdir)/Ssh.bin
	$(top_srcdir)/mkinstalldirs $(DESTDIR)$(datadir)
	$(INSTALL) -m 0644 $(srcdir)/Ssh.bin $(DESTDIR)$(datadir)/Ssh.bin
