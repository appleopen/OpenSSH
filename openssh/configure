  withval="$with_prngd_port"

		case "$withval" in
		no)
			withval=""
			;;
		[0-9]*)
			;;
		*)
			{ { echo "$as_me:7407: error: You must specify a numeric port number for --with-prngd-port" >&5
echo "$as_me: error: You must specify a numeric port number for --with-prngd-port" >&2;}
   { (exit 1); exit 1; }; }
			;;
		esac
		if test ! -z "$withval" ; then
			PRNGD_PORT="$withval"
